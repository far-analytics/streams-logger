{
  "name": "test",
  "version": "1.0.0",
  "description": "",
  "main": "./build/cjs/dist/index.js",
  "type": "module",
  "exports": {
    ".": {
      "import": "./build/esm/dist/index.js",
      "require": "./build/cjs/dist/index.js"
    }
  },
  "scripts": {
    "clean": "rm -rf ./build/cjs/dist/* && rm -rf ./build/esm/dist/*",
    "mark-cjs": "node -e \"const fs=require('node:fs'); fs.writeFileSync('build/cjs/dist/package.json', JSON.stringify({type:'commonjs'}));\"",
    "build": "tsc -p tsconfig.esm.json && tsc -p tsconfig.cjs.json && npm run mark-cjs",
    "clean:build": "npm run clean && npm run build",
    "test:esm": "node --expose-gc ./build/esm/dist",
    "test:cjs": "node --expose-gc ./build/cjs/dist",
    "monitor": "nodemon ."
  },
  "author": "FAR",
  "license": "MIT",
  "engines": {
    "npm": ">=10.2.3",
    "node": ">=18.20.4"
  },
  "dependencies": {
    "streams-logger": "file:.."
  },
  "devDependencies": {
    "@types/node": "^20.6.2",
    "typescript": "^5.1.6",
    "nodemon": "^3.0.1"
  }
}
